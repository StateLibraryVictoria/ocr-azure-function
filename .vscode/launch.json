{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        

        {
            "name": "Attach to Python Functions",
            "type": "debugpy",
            "request": "attach",
            "processId" : "${command:pickProcess}",
            "preLaunchTask": "func: host start"
        }
    ]
}